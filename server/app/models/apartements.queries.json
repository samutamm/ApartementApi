{
  "createTable": [
    "CREATE TABLE IF NOT EXISTS apartements (",
      "id    SERIAL PRIMARY KEY,",
      "name  TEXT,",
      "address TEXT,",
      "city TEXT,",
      "agent TEXT,",
      "price INTEGER",
      " );"
  ],
  "selectByName": [
    "SELECT * FROM apartements",
    " WHERE name = ($1)"
  ],
  "selectAll": [
    "SELECT * FROM apartements;"
  ],
  "addApartement": [
    "INSERT INTO apartements VALUES",
    " (DEFAULT, $1, $2, $3, $4, $5);"
  ]
}
